<?xml version="1.0" ?>
<odoo>
    <record id="agreement_dynamic_alias_tree" model="ir.ui.view">
        <field name="name">agreement.dynamic.alias.tree</field>
        <field name="model">agreement.dynamic.alias</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="expression_from" />
                <field name="expression_to" />
                <field name="is_active" />
            </tree>
        </field>
    </record>
    <record id="agreement_dynamic_alias_action" model="ir.actions.act_window">
        <field name="name">Aliases</field>
        <field name="res_model">agreement.dynamic.alias</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem
        name="Aliases"
        id="agreement_dynamic_alias_menu_root"
        parent="agreement.agreement_menu_root"
        sequence="500"
        action="agreement_dynamic_alias_action"
    />
</odoo>
