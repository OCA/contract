<?xml version="1.0" encoding="utf-8"?>

<odoo>

    <record id="agreement_account.market1" model="agreement">
        <field name="partner_invoice_id" ref="base.res_partner_1"/>
    </record>

    <record id="sale_partner_agreement" model="sale.order">
        <field name="partner_id" ref="base.res_partner_12"/>
        <field name="agreement_id" ref="agreement_account.market2"/>
        <field name="user_id" ref="base.user_demo"/>
        <field name="team_id" ref="sales_team.team_sales_department"/>
        <field name="date_order" eval="(DateTime.today() - relativedelta(months=1)).strftime('%Y-%m-%d %H:%M')"/>
    </record>
    <record id="sale_line_partner_agreement" model="sale.order.line">
        <field name="order_id" ref="sale_partner_agreement"/>
        <field name="name">Laptop E5023</field>
        <field name="product_id" ref="product.product_product_25"/>
        <field name="product_uom_qty">3</field>
        <field name="product_uom" ref="product.product_uom_unit"/>
        <field name="price_unit">2950.00</field>
    </record>

    <record id="sale_partner_agreement" model="sale.order">
        <field name="agreement_id" ref="agreement_account.market1"/>
    </record>
        

</odoo>
