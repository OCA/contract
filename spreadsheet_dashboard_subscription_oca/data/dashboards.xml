<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="spreadsheet_dashboard_subscription_oca" model="spreadsheet.dashboard">
        <field name="name">Subscription</field>
        <field
            name="data"
            type="base64"
            file="spreadsheet_dashboard_subscription_oca/data/files/Subscription.json"
        />
        <field
            name="dashboard_group_id"
            ref="spreadsheet_dashboard.spreadsheet_dashboard_group_sales"
        />
        <field
            name="group_ids"
            eval="[Command.link(ref('sales_team.group_sale_manager'))]"
        />
        <field name="sequence">100</field>
    </record>
</odoo>
